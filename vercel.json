{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api-docs/swagger-ui.*",
            "headers": {
                "Content-Type": "text/css"
            },
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ]
}
